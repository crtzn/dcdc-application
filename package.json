{
  "name": "dental-clinic",
  "private": true,
  "version": "0.0.0",
  "type": "module",
  "main": "dist-electron/main.js",
  "scripts": {
    "dev": "npm-run-all --parallel dev:react dev:electron",
    "rebuild": "electron-rebuild -f -w better-sqlite3",
    "init-db": "npx ts-node src/lib/database.ts",
    "dev:react": "vite",
    "dev:electron": "yarn run transpile:electron; cross-env NODE_ENV=development electron .",
    "transpile:electron": "tsc --project src/electron/tsconfig.json",
    "build": "tsc -b && vite build",
    "lint": "eslint .",
    "preview": "vite preview",
    "dist:mac": "yarn run transpile:electron && yarn run build && electron-builder --mac --arm64",
    "dist:win": "yarn run transpile:electron && yarn run build && electron-builder --win --x64",
    "dist:linux": "yarn run transpile:electron && yarn run build && electron-builder --linux --x64",
    "test:db": "yarn ts-node /src/lib/database.ts"
  },
  "dependencies": {
    "@hookform/resolvers": "^5.0.1",
    "@radix-ui/react-checkbox": "^1.1.4",
    "@radix-ui/react-dialog": "^1.1.6",
    "@radix-ui/react-dropdown-menu": "^2.1.6",
    "@radix-ui/react-label": "^2.1.2",
    "@radix-ui/react-popover": "^1.1.6",
    "@radix-ui/react-radio-group": "^1.2.3",
    "@radix-ui/react-scroll-area": "^1.2.3",
    "@radix-ui/react-select": "^2.1.6",
    "@radix-ui/react-slot": "^1.1.2",
    "@radix-ui/react-tooltip": "^1.1.8",
    "@react-hook/window-size": "^3.1.1",
    "@tailwindcss/vite": "^4.0.15",
    "@types/better-sqlite3": "^7.6.13",
    "@types/os-utils": "^0.0.4",
    "better-sqlite3": "^11.9.1",
    "class-variance-authority": "^0.7.1",
    "clsx": "^2.1.1",
    "cross-env": "^7.0.3",
    "date-fns": "^4.1.0",
    "electron-rebuild": "^3.2.9",
    "lucide-react": "^0.487.0",
    "next-themes": "^0.4.6",
    "npm-run-all": "^4.1.5",
    "os-utils": "^0.0.14",
    "react": "^19.0.0",
    "react-day-picker": "8.10.1",
    "react-dom": "^19.0.0",
    "react-hook-form": "^7.55.0",
    "react-phone-input-2": "^2.15.1",
    "react-router-dom": "^7.4.0",
    "sonner": "^2.0.1",
    "sqlite3": "^5.1.7",
    "tailwind-merge": "^3.2.0",
    "tailwindcss": "^4.0.15",
    "tw-animate-css": "^1.2.5",
    "vite-plugin-electron": "^0.29.0",
    "window-size": "^1.1.1",
    "zod": "^3.24.2"
  },
  "devDependencies": {
    "@eslint/js": "^9.21.0",
    "@types/node": "^22.14.0",
    "@types/react": "^19.0.10",
    "@types/react-dom": "^19.0.4",
    "@vitejs/plugin-react": "^4.3.4",
    "electron": "^35.0.3",
    "electron-builder": "^25.1.8",
    "eslint": "^9.21.0",
    "eslint-plugin-react-hooks": "^5.1.0",
    "eslint-plugin-react-refresh": "^0.4.19",
    "globals": "^15.15.0",
    "ts-node": "^10.9.2",
    "tsconfig-paths": "^4.2.0",
    "typescript": "^5.8.3",
    "typescript-eslint": "^8.24.1",
    "vite": "^6.2.0"
  }
}
